{
  "name": "weatherapp",
  "version": "1.0.0",
  "description": "Weatherapp - A full-stack SPA integrating weather, currency, and quotes utilities",
  "scripts": {
    "install-all": "cd server && npm install && cd ../client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm start",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "weather",
    "currency",
    "quotes",
    "fullstack"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
